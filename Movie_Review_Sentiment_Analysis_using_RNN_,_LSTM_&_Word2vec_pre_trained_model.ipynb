    {
      "cell_type": "code",
      "source": [
        "def clean_text(text):\n",
        "  text=text.lower()\n",
        "  text=re.sub(r'[^a-z0-9\\s]','',text) #removal of punctuation\n",
        "  text=text.split()\n",
        "\n",
        "  stopwords_removed=[] # removal of stopwords\n",
        "  for word in text:\n",
        "    if word not in sw:\n",
        "      stopwords_removed.append(word)\n",
        "\n",
        "  stemmed_words=[] #Stemming\n",
        "  for word in stopwords_removed:\n",
        "    stemmed_words.append(ps.stem(word))\n",
        "\n",
        "  return ' '.join(stemmed_words)"
      ],
      "metadata": {
        "id": "wVzDlYNaI8e3"
      },
      "execution_count": 8,
      "outputs": []
